/***************动画********************/
.invitationFirstOne_tlt1{
	background-image: -webkit-linear-gradient(left, #551524, #E6D205 25%, #147B96  50%, #E6D205 75%, #551524);
    -webkit-text-fill-color: transparent;
    -webkit-background-clip: text;
    -webkit-background-size: 200% 100%;
    -webkit-animation: masked-animation 4s infinite linear;
}
.invitationFirstOne_tlt2{
    background-image: -webkit-linear-gradient(left, #551524, #E6D205 25%, #147B96  50%, #E6D205 75%, #551524);
    -webkit-text-fill-color: transparent;
    -webkit-background-clip: text;
    -webkit-background-size: 200% 100%;
    -webkit-animation: masked-animation 4s infinite linear;
  }
@-webkit-keyframes masked-animation {
     0%{ background-position: 0 0;}
     100% { background-position: -100% 0;}
}
@keyframes fadeInUp {
    from {
        transform: translate3d(0,60rpx,0);
        opacity: 0;
    }

    to {
        transform: translate3d(0,0,0);
        opacity: 1
    }
}

@-webkit-keyframes fadeInUp {
    from {
        transform: translate3d(0,100rpx,0);
        opacity: 0;
    }

    to {
        transform: translate3d(0,0,0);
        opacity: 1;
    }
}
@keyframes fadeInDown {
    from {
        transform: translate3d(0,-100rpx,0);
        opacity: 0;
    }

    to {
        transform: translate3d(0,0,0);
        opacity: 1;
    }
}

@-webkit-keyframes fadeInDown {
    from {
        transform: translate3d(0,-100rpx,0);
        opacity: 0
    }

    to {
        transform: translate3d(0,0,0);
        opacity: 1
    }
}
@keyframes fadeInLeft {
    from {
        transform: translate3d(40rpx,0rpx,0);
        opacity:0;
    }

    to {
        transform: translate3d(0,0,0);
        opacity: 1
    }
}

@-webkit-keyframes fadeInLeft {
    from {
        transform: translate3d(40rpx,0rpx,0);
        opacity:0;
    }

    to {
        transform: translate3d(0,0,0);
        opacity: 1;
    }
}
@keyframes fadeInRight {
    from {
        transform: translate3d(-40rpx,0rpx,0);
        opacity: 0
    }

    to {
        transform: translate3d(0,0,0);
        opacity: 1;
    }
}

@-webkit-keyframes fadeInRight {
    from {
        transform: translate3d(-40rpx,0rpx,0);
        opacity: 0;
    }

    to {
        transform: translate3d(0,0,0);
        opacity: 1;
    }
}
.animated {
    animation-duration: 1s;
    animation-fill-mode: both;
    -webkit-animation-duration: 1s;
    -webkit-animation-fill-mode: both
}
.animated2 {
    animation-duration: 1s;
    animation-fill-mode: both;
    -webkit-animation-duration: 1s;
    -webkit-animation-fill-mode: both
}
.animated1 {
