}
.fadeLeft{
	animation: fadeInLeft 0.5s linear;
	-webkit-animation: fadeInLeft 0.5s linear;
}
.fadeRight5{
	animation: fadeInRight 5s linear;
	-webkit-animation: fadeInRight 5s linear;
}
.fadeRight3{
	animation: fadeInRight 3s linear;
