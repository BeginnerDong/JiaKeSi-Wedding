/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	overflow:hidden;
}
.color{
	color:#490112;
}
.invitation_top{
	width:100%;
	height:280rpx;
	line-height:280rpx;
}
.invitation_left{
	width:60%;
	text-align: center;
	line-height:280rpx;
	height:100%;
}
.invitation_right{
	width:40%;
	line-height:280rpx;
	height:100%;
}
.invitation_left image{
	width:204rpx;
	height:180rpx;
	margin:0 auto;
}
.invitation_right{
	text-align: right;
}
.invitation_right image{
	width:66rpx;
	height:196rpx;
	margin-top: 40rpx;
}
.invitation_center image{
	width:100%;
	height:440rpx;
}
.invitation_item{
	width:222rpx;
	height:280rpx;
}
.invitation_item image{
	width:100%;
	height:100%;
}
.invitation_bottom{
	margin-top:40rpx;
}
.save{
	width:120rpx;
	height:120rpx;
	border-radius:50%;
	text-align: center;
	line-height:120rpx;
	overflow: hidden;
	position: fixed;
	right:0;
	bottom:20%;
}