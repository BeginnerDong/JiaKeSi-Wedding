/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	overflow:hidden;
}
.login{
	width:100%;
	height:100vh;
	position: relative;
}
.login image{
	width:100%;
	height:100%;
}
.login_box{
	width:100%;
	height:400rpx;
	text-align:center;
}
.login_cont{
	position: absolute;
	left:0;
	top:0;
	height:100%;
}
.login_input{
	width:72%;
	margin:0 auto;
}
.merrage{
	width:130rpx;
	height:130rpx;
	border-radius:50%;
	background:#fff;
	text-align:center;
	line-height:130rpx;
	font-size:24rpx;
	margin:0rpx auto;
}