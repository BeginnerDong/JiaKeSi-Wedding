/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	
}
.user_main{
	box-shadow: 0 0 20rpx #f2f2f2;
	padding-top:10rpx;
	padding-bottom:10rpx;
	border-radius:10rpx;
}
.user_list{
	width:100%;
	height:80rpx;
	border:solid 1px #e0e0e0;
	border-radius:10rpx;
	margin:32rpx 0;
}

.user_list input{
	width:94%;
	padding:0 3%;
	border:0;
	font-size:28rpx;
}
.submit_info{
	width:100%;
	position:fixed;
	left:0;
	bottom:0;
	border-radius: 0;
	margin:0;
}
.get_info{
	width:160rpx;
	height:50rpx;
	text-align:center;
	line-height:50rpx;
	font-size:24rpx;
	color:#fff;
}
.user_list textarea{
	width:94%;
	height:260rpx;
	padding:3%;
	font-size:28rpx;
}