<!--index2.wxml-->
<block wx:for="{{web_mainData}}" wx:key="id">
	<view class="w690">
	  <view class="notice" bindtap="intoPath" data-path="/pages/userInvitationNotice/userInvitationNotice?id={{item.id}}">
	    <view class="bride_notice">
	      <image src="{{item.mainImg[0].url}}"></image>
	    </view>
	    <view class="font28 color2" style="height:60rpx;line-height:60rpx;">{{item.title}}</view>
	    <view class="notice_cont font28 color6 avoidOverflow2">
	      {{item.description}}
	    </view>
	  </view>
	</view>
</block>