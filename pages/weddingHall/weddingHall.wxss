/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	background:#fff;
}
.search{
	width:100%;
	height:68rpx;
	border-bottom:solid 1px #f4f4f4;
}
.search_item{
	width:50%;
}

.search_item image{
	width:20rpx;
	height:12rpx;
	margin-left:20rpx;
}
/************列表**************/
.product{
	height:220rpx;
}
.product_box{
	width:100%;
	height:160rpx;

}
.product_detail{
	width:100%;
	height:100rpx;
}
.product_left{
	width:40%;
	height:160rpx;
}
.product_left image{
	width:240rpx;
	height:160rpx;
}
.product_right{
	width:60%;
	height:160rpx;
}
.product_tilt{
	width:100%;
	margin-bottom:30rpx;
}
.product_tilt image{
	width:28rpx;
	height:28rpx;
	margin-right:20rpx;
}
.bottom_line{
	width:100%;
	height:4rpx;
	margin:0;
	position:relative;
	top:-30rpx;
}
.show_list{
	height:230rpx;
}